  "executionRoleArn": "arn:aws:iam::894185188610:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "fintechimage",
      "image": "894185188610.dkr.ecr.ap-south-1.amazonaws.com/new-repo:${SHORT_SHA}",
