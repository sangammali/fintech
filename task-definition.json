{
  "family": "new-fintech",
  "taskRoleArn": "arn:aws:iam::894185188610:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::894185188610:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "fontech",
      "image": "894185188610.dkr.ecr.ap-south-1.amazonaws.com/new-repo:latest",
      "cpu": 256,
      "memory": 512,  // Adjusted to match the container memory limit
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8001,
          "protocol": "tcp",
          "containerPort": 8001
        }
      ]
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512"  // Adjusted to match the container memory limit
}
